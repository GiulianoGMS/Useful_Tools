-- Linha para Command Window no PLSQL a ser formada no Excel

="UPDATE MAP_FAMEMBALAGEM X
   SET DATAHORAALTERACAO = SYSDATE, USUARIOALTERACAO = 'TKT379994', ALTURA = "&C2&" , LARGURA = "&D2&" , PROFUNDIDADE = "&E2&" WHERE SEQFAMILIA =(SELECT SEQFAMILIA FROM MAP_PRODUTO P WHERE P.SEQPRODUTO =(SELECT SEQPRODUTO FROM MAP_PRODCODIGO Z WHERE CODACESSO = '"&A2&"'))AND X.QTDEMBALAGEM = 1;"
